<?xml version="1.0" encoding="UTF-8"?>
<entidad xmlns="http://jnum.com/entidad"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jnum.com/entidad
	../../esquemas/entidad.xsd">
  <modulo>territorio</modulo>
  <nombre>comunidad</nombre>
  <nombreTabla>COMUNIDAD</nombreTabla>
  <label>Comunidad</label>
  <menuLabel>Comunidades Autónomas</menuLabel>
  <nombreID>id_comunidad</nombreID>
  <comboLabel>{nombre}</comboLabel>
  <orden>
    <atributo>nombre</atributo>
  </orden>
  <opciones>
  </opciones>
  <favorito>
  </favorito>
  <embeddedId>
    <atributo>idEntidad</atributo>
    <atributo>idComunidad</atributo>  
  </embeddedId>
  <atributos>
    <!--atributo>
        <nombre>idEntidad</nombre>
        <nombreColumna>ID_ENTIDAD</nombreColumna>
        <obligatorio>true</obligatorio>
        <label>Entidad</label>
        <clase>entidad</clase>
        <esRelacion>true</esRelacion>
        <edicion>
            <tipo>combo</tipo>
            <validaciones>
                <validacion>
                    <nombre>required</nombre>
                </validacion>
            </validaciones>
        </edicion>
        <secciones>
            <insercion>true</insercion>
            <busqueda>
                <operador>eq</operador>
            </busqueda>
            <listado>true</listado>
            <modificacion>true</modificacion>
        </secciones>
    </atributo-->
    <atributo>
      <nombre>idEntidad</nombre>
      <transient>true</transient>
      <obligatorio>true</obligatorio>
      <clase>java.lang.Integer</clase>
    </atributo>

    <atributo>
      <nombre>idComunidad</nombre>
      <transient>true</transient>
      <obligatorio>true</obligatorio>
      <clase>java.lang.Integer</clase>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
      </secciones>
    </atributo>
    <atributo>
      <nombre>codigo</nombre>
      <nombreColumna>codigo</nombreColumna>
      <longitud>2</longitud>
      <obligatorio>true</obligatorio>
      <label>Código</label>
      <clase>java.lang.String</clase>
      <unico>true</unico>
       <events>
        <change>codigoChange()</change>
      </events>
      <edicion>
        <tipo>text</tipo>
        <longitud>2</longitud>
        <validaciones>
          <validacion>
            <nombre>positive</nombre>
          </validacion>
          <validacion>
            <nombre>maxlength</nombre>
            <valor>2</valor>
          </validacion>
          <validacion>
            <nombre>required</nombre>
          </validacion>
          <validacion>
            <nombre>number</nombre>
          </validacion>
        </validaciones>
        <validacionessearch>
          <validacion>
            <nombre>positive</nombre>
          </validacion>
        </validacionessearch>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    <atributo>
      <nombre>nombre</nombre>
      <nombreColumna>nombre</nombreColumna>
      <longitud>100</longitud>
      <obligatorio>true</obligatorio>
      <label>Nombre</label>
      <clase>java.lang.String</clase>
      <edicion>
        <tipo>text</tipo>
        <longitud>100</longitud>
        <validaciones>
          <validacion>
            <nombre>required</nombre>
          </validacion>
          <validacion>
            <nombre>maxlength</nombre>
            <valor>100</valor>
          </validacion>
        </validaciones>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    <atributo>
      <nombre>organo</nombre>
      <nombreColumna>organo</nombreColumna>
      <longitud>100</longitud>
      <obligatorio>false</obligatorio>
      <label>Nombre Órgano Gobierno</label>
      <clase>java.lang.String</clase>
      <edicion>
        <tipo>text</tipo>
        <longitud>100</longitud>
        <validaciones>
          <validacion>
            <nombre>maxlength</nombre>
            <valor>100</valor>
          </validacion>
        </validaciones>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
  </atributos>
  <visual>
    <busqueda>
      <vista>
        <definicion>void.nombreeq(width=7)</definicion>
      </vista>
    </busqueda>
    <adicion>
      <agregable>true</agregable>
      <vista>
        <definicion>this.codigo(width=4); this.nombre(width=7); this.organo(width=7)</definicion>
     </vista>
    </adicion>
    <edicion>
      <editable>true</editable>
      <vista>
        <definicion>this.codigo(width=4); this.nombre(width=7); this.organo(width=7)</definicion>
      </vista>
    </edicion>
    <listado>
      <deletable>true</deletable>
      <vista>
        <definicion>codigo, nombre, organo</definicion>
      </vista>
    </listado>
    <propselect>
      <vista>
        <definicion>void.codigoeq, void.nombreeq, void.organoeq</definicion>
      </vista>
    </propselect>
  </visual>
</entidad>