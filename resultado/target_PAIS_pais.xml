<?xml version="1.0" encoding="UTF-8"?>
<entidad xmlns="http://jnum.com/entidad"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jnum.com/entidad
	../../esquemas/entidad.xsd">
  <modulo>territorio</modulo>
  <nombre>pais</nombre>
  <nombreTabla>PAIS</nombreTabla>
  <label>País</label>
  <menuLabel>Países</menuLabel>
  <nombreID>id_pais</nombreID>
  <comboLabel>{nombre}</comboLabel>
  <orden>
    <atributo>nombre</atributo>
  </orden>
  <opciones>
  </opciones>
  <favorito>
  </favorito>
  <embeddedId>
    <atributo>idEntidad</atributo>
    <atributo>idPais</atributo>
  </embeddedId>
  <atributos>
    <atributo>
      <nombre>idEntidad</nombre>
      <transient>true</transient>
      <obligatorio>true</obligatorio>
      <clase>java.lang.Integer</clase>
    </atributo>

    <atributo>
      <nombre>idPais</nombre>
      <transient>true</transient>
      <obligatorio>true</obligatorio>
      <clase>java.lang.Integer</clase>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>codigo</nombre>
      <nombreColumna>codigo</nombreColumna>
      <longitud>4</longitud>
      <obligatorio>true</obligatorio>
      <label>Código</label>
      <clase>java.lang.String</clase>
      <unico>true</unico>
      <events>
        <change>codigoChange()</change>
      </events>
      <edicion>
        <tipo>text</tipo>
        <longitud>4</longitud>
        <validaciones>
          <validacion>
            <nombre>maxlength</nombre>
            <valor>4</valor>
          </validacion>
          <validacion>
            <nombre>required</nombre>
          </validacion>
        </validaciones>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>digito</nombre>
      <nombreColumna>digito</nombreColumna>
      <obligatorio>false</obligatorio>
      <label>Dígito</label>
      <clase>java.lang.Integer</clase>
      <edicion>
        <tipo>text</tipo>
        <validaciones>
          <validacion>
            <nombre>number</nombre>
          </validacion>
        </validaciones>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>nombre</nombre>
      <nombreColumna>nombre</nombreColumna>
      <longitud>100</longitud>
      <obligatorio>true</obligatorio>
      <label>Nombre</label>
      <clase>java.lang.String</clase>
      <edicion>
        <tipo>text</tipo>
        <longitud>100</longitud>
        <validaciones>
          <validacion>
            <nombre>required</nombre>
          </validacion>
          <validacion>
            <nombre>maxlength</nombre>
            <valor>100</valor>
          </validacion>
        </validaciones>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>comunitario</nombre>
      <nombreColumna>comunitario</nombreColumna>
      <obligatorio>false</obligatorio>
      <label>Comunitario</label>
      <clase>java.lang.Boolean</clase>
      <edicion>
        <tipo>checkbox</tipo>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>activo</nombre>
      <nombreColumna>activo</nombreColumna>
      <obligatorio>false</obligatorio>
      <label>Activo</label>
      <clase>java.lang.Boolean</clase>
      <edicion>
        <tipo>checkbox</tipo>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
    
    <atributo>
      <nombre>asimiladoAComunitario</nombre>
      <nombreColumna>asimiladoAComunitario</nombreColumna>
      <obligatorio>false</obligatorio>
      <label>Asimilado a Comunitario</label>
      <clase>java.lang.Boolean</clase>
      <edicion>
        <tipo>checkbox</tipo>
      </edicion>
      <secciones>
        <busqueda>
          <operador>eq</operador>
        </busqueda>
        <listado>true</listado>
        <insercion>true</insercion>
        <modificacion>true</modificacion>
      </secciones>
    </atributo>
  </atributos>
  
  <visual>
    <busqueda>
      <vista>
        <definicion>void.nombreeq(width=7)</definicion>
      </vista>
    </busqueda>
    <adicion>
      <agregable>true</agregable>
      <vista>
        <definicion>this.codigo(width=4); this.digito(width=2); this.nombre(width=7); this.comunitario(width=3); this.activo(width=2); this.asimiladoAComunitario(width=3)</definicion>
     </vista>
    </adicion>
    <edicion>
      <editable>true</editable>
      <vista>
        <definicion>this.codigo(width=4); this.digito(width=2); this.nombre(width=7); this.comunitario(width=3); this.activo(width=2); this.asimiladoAComunitario(width=3)</definicion>
      </vista>
    </edicion>
    <listado>
      <deletable>true</deletable>
      <vista>
        <definicion>codigo, digito, nombre, comunitario, activo, asimiladoAComunitario</definicion>
      </vista>
    </listado>
    <propselect>
      <vista>
        <definicion>void.codigoeq, void.digitoeq, void.nombreeq, void.comunitarioeq, void.activoeq, void.asimiladoAComunitarioeq</definicion>
      </vista>
    </propselect>
  </visual>
</entidad> 